{
  "name": "smart-forest",
  "version": "1.0.0",
  "description": "GIS-based forestry platform",
  "scripts": {
    "build": "npx nest build",
    "start": "npx nest start",
    "start:9001": "cross-env PORT=9001 npx nest start",
    "start:dev": "npx nest start --watch",
    "start:dev:9001": "cross-env PORT=9001 npx nest start --watch",
    "camera:cam1": "node web/scripts/camera-cam1.js",
    "dev": "npm run dev --prefix web",
    "dev:all": "concurrently -n backend,web -c blue,green \"npm run start:dev\" \"npm run dev:custom --prefix web\"",
    "dev:full": "docker-compose up -d && npm run dev:all",
    "start:debug": "npx nest start --debug --watch",
    "start:prod": "node dist/main",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d src/infrastructure/database/data-source.ts",
    "seed:uzbekistan-boundary": "ts-node -r tsconfig-paths/register scripts/seed-uzbekistan-boundary.ts",
    "seed:events": "ts-node -r tsconfig-paths/register scripts/seed-events.ts",
    "import:geojson": "ts-node -r tsconfig-paths/register scripts/import-geojson-to-postgis.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "^10.0.0",
    "@turf/turf": "^7.3.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.0",
    "helmet": "^7.2.0",
    "mqtt": "^5.14.1",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.0",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/node": "^20.0.0",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "ts-node": "^10.9.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.0"
  }
}
